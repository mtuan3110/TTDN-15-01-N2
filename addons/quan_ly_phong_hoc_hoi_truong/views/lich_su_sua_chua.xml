<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="lich_su_sua_chua_form" model="ir.ui.view">
            <field name="name">lich_su_sua_chua.form</field>
            <field name="model">lich_su_sua_chua</field>
            <field name="arch" type="xml">
                <form string="Lịch sử sửa chữa thiết bị">
                    <sheet>
                        <group>
                            <field name="equipment_id"/>
                            <field name="repair_date"/>
                            <field name="repaired_by"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_lich_su_sua_chua" model="ir.actions.act_window">
            <field name="name">Lịch sử sửa chữa</field>
            <field name="res_model">lich_su_sua_chua</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>
        
    